   "source": [
    "### Analyse population data "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "comic-designation",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Add new columns for total people displaced and total people displaced abroad\n",
    "\n",
    "population['total'] = population['Refugees under UNHCR\\'s mandate'].fillna(0) \\\n",
    "                    + population['Asylum-seekers'].fillna(0) + population['IDPs of concern to UNHCR'].fillna(0) \\\n",
    "                    + population['Venezuelans displaced abroad'].fillna(0) + population['Stateless persons'].fillna(0) \\\n",
    "                    + population['Others of concern'].fillna(0)\n",
    "\n",
    "# Total abroad includes: refugees, asylum seekers, Venezuelans displaced abroad\n",
    "population['total_abroad'] = population['Refugees under UNHCR\\'s mandate'].fillna(0) \\\n",
    "                    + population['Asylum-seekers'].fillna(0) + population['Venezuelans displaced abroad'].fillna(0)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "aquatic-cement",
   "metadata": {},
   "outputs": [
